{
    "name": "vagrant/delivery-web.install",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "files": [
            "app/AppKernel.php"
        ]
    },
	"repositories":
    [
        {
            "type": "vcs",
            "url": "https://github.com/we-bridge/symfony2-test-helpers"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "3.*",
        "doctrine/orm": "~2.5",
        "doctrine/dbal": "~2.5",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/migrations": "~1.4",
        "doctrine/doctrine-migrations-bundle": "~1.1",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "symfony/assetic-bundle": "~2.8",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.10",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "~2.1",
        "twig/extensions": "~1.3",
	    "friendsofsymfony/user-bundle": "dev-master",
        "knplabs/knp-paginator-bundle": "~2.5",
        "mashape/unirest-php": "3.*",
        "jms/translation-bundle": "~1.2,>=1.2.3",
        "fortawesome/font-awesome": "4.5.*",
        "twbs/bootstrap": "3.3.*",
        "components/jquery": "2.2.*",
        "onokumus/metismenu": "2.4.*",
        "components/flag-icon-css": "~1.0",
        "leafo/scssphp": "^0.6.5"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "we-bridge/functionnal-test-helpers": ">=0.0.9",
        "liip/functional-test-bundle": "~1.5",
        "fabpot/php-cs-fixer": "^1.11"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "secure-http": false
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-tests-dir": "tests",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
